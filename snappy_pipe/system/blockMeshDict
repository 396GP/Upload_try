(
    (-1 -1 -1.0) // min x,y,z
    (1 -1 -1.0) // +x
    (1 1 -1.0) // + y
    (-1 1 -1.0) // -x
    (-1 -1 41.0)// +z
    (1 -1 41.0) // -y
    (1 1 41.0) // +y
    (-1 1 41.0) // -x
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 100) simpleGrading (1 1 1)
);

